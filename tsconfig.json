{
  "compilerOptions": {
    "target": "es2016",
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true
  },
  "include": [
    "build/**/*.ts",
    "types/**/*.ts",
    "views_V2024/**/*.ts",
    "w/**/*.ts",
    "type.d.ts"
  ]
}
